{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"rootDir": ".",
		"target": "es5",
		"sourceMap": true,
		"strictNullChecks": true,
		"noUnusedLocals": true,
		"esModuleInterop": true,
		"outDir": "dist",
		"declaration": true,
		"lib": ["es2015", "es2016", "dom", "esnext.asynciterable"]
	},
	"include": ["lib"],
	"exclude": ["node_modules", "dist"]
}
